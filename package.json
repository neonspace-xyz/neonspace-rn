    "lodash.debounce": "^4.0.8",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "neonspace-app": "file:",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
